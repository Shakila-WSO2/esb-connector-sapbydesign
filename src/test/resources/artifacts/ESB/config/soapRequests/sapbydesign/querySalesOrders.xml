<soap:Envelope xmlns:soap="http://www.w3.org/2003/05/soap-envelope">
    <soap:Header/>
    <soap:Body>
        <querySalesOrders>
            <username>%s(username)</username>
            <password>%s(password)</password>
            <sapHost>%s(sapHost)</sapHost>
            <blocking>%s(blocking)</blocking>
            <queryObject>
                <SalesOrderSelectionByElements>
                    <SelectionByID>
                        <IntervalBoundaryTypeCode>1</IntervalBoundaryTypeCode>
                        <LowerBoundaryID>%s(salesOrderInternalId)</LowerBoundaryID>
                    </SelectionByID>
                </SalesOrderSelectionByElements>
            </queryObject>
            <method>querySalesOrders</method>
        </querySalesOrders>
    </soap:Body>
</soap:Envelope>